<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ModificarFrenos">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Modificacion de frenos "
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#0D0000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="225dp"
        android:layout_height="211dp"
        android:layout_marginTop="52dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_editor_absoluteX="93dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="163dp"
        android:layout_height="28dp"
        android:layout_marginTop="76dp"
        android:text="Tipos de frenos"
        android:textAlignment="center"
        android:textColor="#0C0C0C"
        android:textIsSelectable="false"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="78dp"
        android:layout_height="26dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="36dp"
        android:text="Pinzas "
        android:textAlignment="center"
        android:textColor="#050505"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Spinner
        android:id="@+id/sp_Pistones"
        android:layout_width="186dp"
        android:layout_height="34dp"
        android:layout_marginTop="24dp"
        android:prompt="@string/seleccionBujia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.626"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/sp_TiposFrenos"
        tools:ignore="MissingConstraints" />

    <Spinner
        android:id="@+id/sp_TiposFrenos"
        android:layout_width="194dp"
        android:layout_height="29dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="76dp"
        android:prompt="@string/seleccionBujia"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

</androidx.constraintlayout.widget.ConstraintLayout>